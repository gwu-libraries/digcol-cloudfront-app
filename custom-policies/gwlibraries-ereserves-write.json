{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ereservesList",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::serverlessrepo-gwu-s3-access-s3bucket-{bucket-id}"
            ]
        },
        {
            "Sid": "ereservesPut",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::serverlessrepo-gwu-s3-access-s3bucket-{bucket-id}/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:us-east-1:{user-id}:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:417560131410:log-group:/aws/lambda/gwlibraries-ereserves-object-list:*"
            ]
        }
    ]
}